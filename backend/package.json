{
  "name": "@aleaac/backend",
  "version": "1.0.0",
  "description": "The full backend",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development NODE_ICU_DATA=node_modules\\full-icu nodemon",
    "startdev": "cross-env NODE_ENV=devserver NODE_ICU_DATA=node_modules/full-icu nodemon",
    "prestart:prod": "tsc",
    "build:prod": "tsc",
    "lint": "tslint 'src/**/*.ts'",
    "start:prod": "cross-env NODE_ENV=production NODE_ICU_DATA=node_modules/full-icu nodemon --exitcrash",
    "test": "cross-env NODE_ENV=test jest --config=jest.json --coverage --runInBand --forceExit",
    "test:watch": "cross-env NODE_ENV=test jest --watch --config=jest.json",
    "test:coverage": "cross-env NODE_ENV=test jest --config=jest.json --coverage --coverageDirectory=coverage",
    "e2e": "cross-env NODE_ENV=test jest --config=e2e/jest-e2e.json",
    "e2e:watch": "cross-env NODE_ENV=test jest --watch --config=e2e/jest-e2e.json",

    "typeorm:local": "ts-node ./node_modules/typeorm/cli.js --config properties/development.ormconfig",
    "typeorm:dev": "ts-node ./node_modules/typeorm/cli.js --config properties/devserveur.ormconfig",
    "typeorm:prod": "ts-node ./node_modules/typeorm/cli.js --config properties/production.ormconfig"
  },
